description="Create tmpfiles.d entries (boot)"

depend()
{
	need localmount
}

start()
{
	ebegin "Creating ${description#Create }"
	@LIBEXECDIR@/sh/tmpfiles.sh --create ${tmpfiles_opts}
	eend $?
	return 0
}
