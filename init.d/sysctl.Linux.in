depend()
{
	use hostname
	before bootmisc logger
	keyword noopenvz noprefix novserver
}

start()
{
	[ -e /etc/sysctl.conf ] || return 0
	ebegin "Configuring kernel parameters"
	sysctl -p >/dev/null
	eend $? "Some errors were encountered"
}
